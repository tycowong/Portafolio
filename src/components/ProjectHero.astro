---
import type { ImageMetadata } from "astro";
import { Image } from "astro:assets";
interface Props {
  img: string;
  alt: string;
}

const { img, alt } = Astro.props;
---

<img
  src=`imgaes/${img}`
  alt=`${alt}`
  class="w-[548px] h-[360px] rounded-md"
  transition:name=`img-${img}`
/>
